{
    "name": "sleek",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint"
    },
    "eslintConfig": {
        "parser": "@typescript-eslint/parser",
        "plugins": [
            "prettier",
            "import"
        ],
        "extends": [
            "eslint:recommended",
            "next/core-web-vitals",
            "prettier",
            "plugin:import/recommended",
            "plugin:@typescript-eslint/recommended"
        ],
        "rules": {
            "no-case-declarations": "off",
            "no-empty-function": "off",
            "prefer-const": "off",
            "prettier/prettier": "error",
            "@next/next/no-img-element": "off",
            "react/no-unescaped-entities": "off",
            "import/order": [
                "error",
                {
                    "groups": [
                        "index",
                        "sibling",
                        "parent",
                        "internal",
                        "external",
                        "builtin",
                        "object",
                        "type"
                    ]
                }
            ]
        },
        "globals": {
            "JSX": true
        }
    },
    "prettier": {
        "trailingComma": "es5",
        "bracketSpacing": true,
        "arrowParens": "always",
        "endOfLine": "auto",
        "htmlWhitespaceSensitivity": "css",
        "bracketSameLine": false,
        "jsxSingleQuote": false,
        "printWidth": 200,
        "semi": true,
        "tabWidth": 4
    },
    "dependencies": {
        "@prisma/client": "^4.8.0",
        "next": "13.0.7",
        "react": "18.2.0",
        "react-dom": "18.2.0"
    },
    "devDependencies": {
        "@types/node": "18.11.17",
        "@types/react": "18.0.26",
        "@types/react-dom": "18.0.9",
        "@typescript-eslint/eslint-plugin": "^5.47.0",
        "@typescript-eslint/parser": "^5.47.0",
        "autoprefixer": "^10.4.13",
        "eslint": "8.30.0",
        "eslint-config-next": "13.0.7",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.2.1",
        "postcss": "^8.4.20",
        "prettier": "^2.8.1",
        "prisma": "^4.8.0",
        "tailwindcss": "^3.2.4",
        "ts-node": "^10.9.1",
        "typescript": "4.9.4"
    }
}
